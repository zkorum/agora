{
    "name": "@zkorum/agora-api",
    "version": "1.0.0",
    "description": "API for Agora Citizen Network",
    "exports": "./index.js",
    "scripts": {
        "build": "NODE_ENV=production rm -rf dist && tsc --sourceMap -p tsconfig.json",
        "start": "node --loader esm-module-alias/loader --loader ./esm-loader-lodash-fix.mjs --no-warnings dist/index.js",
        "start:dev": "tsx watch --trace-uncaught src/index.ts",
        "start:import": "tsx src/index.ts",
        "start:dev:capacitor": "MODE=capacitor tsx watch src/index.ts",
        "db:drop": "drizzle-kit drop",
        "db:generate": "cd ../shared-backend && pnpm run sync && cd ../api && drizzle-kit generate && ./scripts/rename_sql_migration_file.sh drizzle/",
        "db:migrate": "docker run --rm --network host -v ${PWD}/database/flyway:/flyway/conf -v ${PWD}/database/flyway:/flyway/sql flyway/flyway migrate",
        "db:undo": "docker run --rm --network host -v ${PWD}/database/flyway:/flyway/conf -v ${PWD}/database/flyway:/flyway/sql flyway/flyway undo",
        "db:sudo:migrate": "sudo docker run --rm --network host -v ${PWD}/database/flyway:/flyway/conf -v ${PWD}/database/flyway:/flyway/sql flyway/flyway  migrate",
        "db:clean": " docker run --rm --network host -v ${PWD}/database/flyway:/flyway/conf -v ${PWD}/database/flyway:/flyway/sql flyway/flyway clean",
        "lint": "eslint  .",
        "format:write": "prettier --write \"**/*.{js,ts,jsx,tsx,vue,scss,html,md,json}\"",
        "format:check": "prettier . --check",
        "generate-dev-keys": "NODE_OPTIONS=--experimental-vm-modules GENERATE_DEV_KEYS=true jest -t 'generate-dev-keys'",
        "test": "./scripts/run-tests.sh",
        "image:buildx": "docker buildx build --platform linux/amd64 -t agora-api --load .",
        "image:build": "docker build -t agora-api ."
    },
    "type": "module",
    "engines": {
        "node": ">=16"
    },
    "keywords": [
        "trust-minimized infrastructure",
        "privacy",
        "transparency",
        "democracy",
        "poll",
        "survey",
        "vote"
    ],
    "author": "ZKorum SAS <hello@zkorum.com>",
    "license": "AGPL-3.0",
    "aliases": {
        "@": "./dist"
    },
    "pnpm": {
        "patchedDependencies": {
            "@zk-kit/eddsa-poseidon@1.1.0": "patches/@zk-kit__eddsa-poseidon@1.1.0.patch"
        },
        "overrides": {
            "fast-xml-parser": ">=5.3.6",
            "brace-expansion": ">=2.0.2",
            "@eslint/plugin-kit": ">=0.3.4",
            "esbuild": ">=0.25.0",
            "valibot": ">=1.2.0",
            "js-yaml": ">=4.1.1",
            "jsonpath": ">=1.2.0"
        }
    },
    "dependencies": {
        "@aws-sdk/client-bedrock-runtime": "^3.995.0",
        "@aws-sdk/client-s3": "^3.995.0",
        "@aws-sdk/client-secrets-manager": "^3.995.0",
        "@aws-sdk/client-ses": "^3.995.0",
        "@aws-sdk/s3-request-presigner": "^3.995.0",
        "@date-fns/tz": "^1.4.1",
        "@fastify/auth": "^5.0.4",
        "@fastify/cors": "^11.2.0",
        "@fastify/multipart": "^9.4.0",
        "@fastify/sensible": "^6.0.4",
        "@fastify/sse": "^0.4.0",
        "@fastify/swagger": "^9.7.0",
        "@google-cloud/translate": "^9.3.0",
        "@pcd/gpc": "^0.4.1",
        "@pcd/pod": "0.5.1",
        "@pcd/proto-pod-gpc-artifacts": "^0.13.0",
        "@sindresorhus/slugify": "^3.0.0",
        "@ucans/ucans": "^0.12.0",
        "@valkey/valkey-glide": "^2.2.7",
        "axios": "^1.13.5",
        "cborg": "^4.5.8",
        "csv-parse": "^5.6.0",
        "date-fns": "^4.1.0",
        "dotenv": "^16.5.0",
        "drizzle-orm": "^0.45.1",
        "esm-module-alias": "^2.2.1",
        "extract-first-json": "^2.0.2",
        "fast-csv": "^5.0.5",
        "fastify": "^5.7.4",
        "fastify-type-provider-zod": "^6.1.0",
        "keystore-idb": "^0.15.5",
        "libphonenumber-js": "^1.12.37",
        "linkify-html": "^4.3.2",
        "linkifyjs": "^4.3.2",
        "localforage": "^1.10.0",
        "lodash": "^4.17.23",
        "multiformats": "^13.4.2",
        "node-cron": "^4.2.1",
        "nodemailer": "^8.0.1",
        "p-limit": "^7.3.0",
        "parse-json-object": "^3.0.1",
        "pg": "^8.18.0",
        "postgres": "^3.4.8",
        "sanitize-html": "^2.17.1",
        "tweetnacl": "^1.0.3",
        "twilio": "^5.12.2",
        "uint8arrays": "^4.0.10",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.3",
        "@eslint/json": "^0.11.0",
        "@eslint/markdown": "^7.5.1",
        "@types/eslint__js": "^8.42.3",
        "@types/lodash": "^4.17.23",
        "@types/node": "^25.3.0",
        "@types/nodemailer": "^6.4.17",
        "@types/pg": "^8.16.0",
        "@types/sanitize-html": "^2.16.0",
        "drizzle-kit": "^0.31.9",
        "eslint": "^9.27.0",
        "eslint-config-prettier": "^10.1.8",
        "globals": "^17.3.0",
        "pino": "10.3.1",
        "pino-pretty": "^13.1.3",
        "prettier": "^3.8.1",
        "testcontainers": "^11.12.0",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.56.0",
        "vite": "^7.1.11",
        "vite-tsconfig-paths": "^6.1.1",
        "vitest": "^4.0.18"
    }
}
