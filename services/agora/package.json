{
  "name": "@zkorum/agora-app",
  "version": "0.0.8",
  "description": "Where citizens converge to exchange and debate ideas.",
  "productName": "Agora Citizen Network",
  "author": "ZKorum SAS <hello@zkorum.com>",
  "license": "MPL-2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue,.md,.json,.html ./",
    "format:write": "prettier --write \"**/*.{js,ts,jsx,tsx,vue,scss,html,md,json}\"",
    "format:check": "prettier . --check",
    "prepare": "quasar prepare",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:android": "quasar dev -m capacitor -T android",
    "build": "quasar build",
    "build:staging": "env-cmd -f .env.staging quasar build",
    "build:production": "env-cmd -f .env.production quasar build",
    "build:ios": "quasar build -m capacitor -T ios --skip-pkg",
    "build:android": "quasar build -m capacitor -T android -- bundleRelease",
    "sign:android": "./scripts/artifact_sign.sh",
    "build:ci:ios": "yarn quasar build -m capacitor -T ios --skip-pkg",
    "image:build": "docker build -f Dockerfile.production -t agora-app .",
    "image:buildx": "docker buildx build -f Dockerfile.production --platform linux/amd64,linux/arm64 -t agora-app --load .",
    "image:build:staging": "docker build -f Dockerfile.staging -t agora-app-staging .",
    "image:buildx:staging": "docker buildx build -f Dockerfile.staging --platform linux/amd64,linux/arm64 -t agora-app-staging --load .",
    "image:build:production": "docker build -f Dockerfile.production -t agora-app-production .",
    "image:buildx:production": "docker buildx build -f Dockerfile.production --platform linux/amd64,linux/arm64 -t agora-app-production --load .",
    "image:tag": "docker tag zkorum-frontJustForExample:latest zkorum/frontJustForExample:0.1.0",
    "image:push": "docker image push zkorum/frontJustForExample:0.1.0"
  },
  "dependencies": {
    "@aparajita/capacitor-biometric-auth": "^8.0.0",
    "@aparajita/capacitor-secure-storage": "^6.0.1",
    "@capgo/capacitor-updater": "^5.9.4",
    "@dicebear/collection": "^9.2.2",
    "@dicebear/core": "^9.2.2",
    "@primevue/themes": "^4.2.5",
    "@quasar/extras": "^1.16.15",
    "@ucans/ucans": "^0.12.0",
    "@vueuse/core": "^13.1.0",
    "@vueuse/integrations": "^13.1.0",
    "@vueuse/router": "^13.1.0",
    "@zkorum/capacitor-secure-signing": "^1.4.0",
    "@zkorum/keystore-idb": "^0.18.1",
    "axios": "^1.7.9",
    "buffer": "^6.0.3",
    "cborg": "^4.2.0",
    "libphonenumber-js": "^1.11.10",
    "multiformats": "^13.3.0",
    "pinia": "^3.0.2",
    "primevue": "^4.2.5",
    "qrcode": "^1",
    "quasar": "^2.18.1",
    "sanitize-html": "^2.14.0",
    "swiper": "^11.2.0",
    "tweetnacl": "^1.0.3",
    "uint8arrays": "^5.1.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.3.0",
    "@html-eslint/eslint-plugin": "^0.39.0",
    "@html-eslint/parser": "^0.39.0",
    "@iconify/vue": "^5.0.0",
    "@quasar/app-vite": "^2.2.0",
    "@types/node": "^22.9.1",
    "@types/sanitize-html": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.4.7",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^3.4.2",
    "typescript": "5.5.4",
    "unplugin-vue-router": "^0.12.0",
    "vite-plugin-checker": "^0.9.1",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
