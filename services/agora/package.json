    "@tiptap/starter-kit": "3.14.0",
    "@tiptap/vue-3": "3.14.0",
    "@ucans/ucans": "^0.12.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/integrations": "^14.1.0",
    "@vueuse/router": "^14.1.0",
    "@zkorum/capacitor-secure-signing": "^1.4.0",
    "@zkorum/keystore-idb": "^0.18.1",
    "axios": "^1.13.5",
    "buffer": "^6.0.3",
    "cborg": "^4.2.15",
    "inapp-spy": "^5.0.7",
    "libphonenumber-js": "^1.12.17",
    "linkify-html": "^4.3.2",
    "localforage": "^1.10.0",
    "linkifyjs": "^4.3.2",
    "localforage": "^1.10.0",
    "maz-ui": "3.50.1",
    "multiformats": "^13.4.1",
    "pinia": "^3.0.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.3",
    "qrcode": "^1",
    "quasar": "^2.18.6",
    "sanitize-html": "^2.16.0",
