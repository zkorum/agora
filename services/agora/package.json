    "uint8arrays": "^5.1.0",
    "vue": "^3.5.25",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.4",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
