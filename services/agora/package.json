{
  "name": "@zkorum/agora-app",
  "version": "0.0.8",
  "description": "Where citizens converge to exchange and debate ideas.",
  "productName": "Agora Citizen Network",
  "author": "ZKorum SAS <hello@zkorum.com>",
  "license": "MPL-2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "lint:fix": "pnpm lint --fix",
    "format:write": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\"",
    "format:check": "prettier . --check",
    "prepare": "quasar prepare",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "start": "serve dist/spa",
    "build:dev": "env-cmd -f .env.dev.build quasar build",
    "build:staging": "env-cmd -f .env.staging quasar build",
    "build:production": "env-cmd -f .env.production quasar build",
    "image:build": "docker build -f Dockerfile.production -t agora-app .",
    "image:buildx": "docker buildx build -f Dockerfile.production --platform linux/amd64 -t agora-app --load .",
    "image:build:staging": "docker build -f Dockerfile.staging -t agora-app-staging .",
    "image:buildx:staging": "docker buildx build -f Dockerfile.staging --platform linux/amd64 -t agora-app-staging --load .",
    "image:build:production": "docker build -f Dockerfile.production -t agora-app-production .",
    "image:buildx:production": "docker buildx build -f Dockerfile.production --platform linux/amd64 -t agora-app-production --load .",
    "image:tag": "docker tag zkorum-frontJustForExample:latest zkorum/frontJustForExample:0.1.0",
    "image:push": "docker image push zkorum/frontJustForExample:0.1.0"
  },
  "dependencies": {
    "@dicebear/collection": "^9.3.2",
    "@dicebear/core": "^9.3.2",
    "@parcnet-js/app-connector": "^1.1.10",
    "@parcnet-js/podspec": "^1.2.0",
    "@parcnet-js/ticket-spec": "^1.1.9",
    "@pcd/gpc": "^0.4.0",
    "@primeuix/themes": "^2.0.3",
    "@quasar/extras": "^1.17.0",
    "@sentry/vue": "^10.39.0",
    "@tanstack/vue-query": "^5.92.9",
    "@tiptap/core": "3.20.0",
    "@tiptap/extension-bubble-menu": "^3.20.0",
    "@tiptap/extension-placeholder": "3.20.0",
    "@tiptap/extensions": "3.20.0",
    "@tiptap/pm": "3.20.0",
    "@tiptap/starter-kit": "3.20.0",
    "@tiptap/vue-3": "3.20.0",
    "@ucans/ucans": "^0.12.0",
    "@vueuse/core": "^14.2.1",
    "@vueuse/integrations": "^14.2.1",
    "@vueuse/router": "^14.2.1",
    "@zkorum/keystore-idb": "^0.18.1",
    "axios": "^1.13.5",
    "buffer": "^6.0.3",
    "cborg": "^4.5.8",
    "inapp-spy": "^5.0.7",
    "libphonenumber-js": "^1.12.37",
    "linkify-html": "^4.3.2",
    "linkifyjs": "^4.3.2",
    "localforage": "^1.10.0",
    "maz-ui": "4.7.1",
    "multiformats": "^13.4.1",
    "pinia": "^3.0.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.3",
    "qrcode": "^1",
    "quasar": "^2.18.6",
    "sanitize-html": "^2.17.1",
    "tweetnacl": "^1.0.3",
    "uint8arrays": "^5.1.0",
    "vue": "^3.5.28",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@eslint/json": "^1.0.1",
    "@iconify/vue": "^5.0.0",
    "@quasar/app-vite": "2.4.1",
    "@sentry/vite-plugin": "^4.9.1",
    "@types/node": "^25.3.0",
    "@types/qrcode": "^1.5.6",
    "@types/sanitize-html": "^2.13.0",
    "@vitejs/plugin-basic-ssl": "^2.1.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.7.0",
    "autoprefixer": "^10.4.24",
    "env-cmd": "^11.0.0",
    "eslint": "^9.35.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.8.0",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "serve": "^14.2.5",
    "terser": "^5.46.0",
    "typescript": "5.9.3",
    "unplugin-vue-router": "^0.19.2",
    "vite": "^7.3.1",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^3.2.5"
  },
  "engines": {
    "node": "^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "@commander-js/extra-typings>commander": "14"
      }
    },
    "overrides": {
      "@eslint/eslintrc>ajv": "^6.12.4",
      "ajv": ">=6.12.3",
      "lodash": ">=4.17.23",
      "valibot": ">=1.2.0",
      "jsonpath": ">=1.2.0"
    }
  }
}
