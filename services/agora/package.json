    "autoprefixer": "^10.4.21",
    "env-cmd": "^11.0.0",
    "eslint": "^9.35.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^17.3.0",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "serve": "^14.2.5",
    "terser": "^5.44.1",
    "typescript": "5.9.3",
    "unplugin-vue-router": "^0.19.2",
    "vite": "^7.3.0",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^3.2.1"
  },
  "engines": {
    "node": "^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
